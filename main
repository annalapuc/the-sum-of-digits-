import java.util.Scanner;

public class Main {

    public static void main(String[] args) {

        System.out.println("Enter the integer:");
        Scanner in= new Scanner(System.in);
        int a=in.nextInt();
        int c=0;
        while (a!=0) {
           c = c+(a%10);
           a=a/10;
        }
        System.out.println("The sum of digits is equal: "+c);
    }
}
